{% extends 'base.html.twig' %}
{% block title %} Task List Items{% endblock %}
{% block body %}


    <div class="row">

        <ul class="collection">
                    <li class="collection-header"><h5></h5></li>
            {% for task in tasks %} 
                        <li class="collection-item">Task title : <a href="{{ path("google_task_api_taskDetails", {"taskList": task.id}) }}">{{ task.title }}</a>
                            </br>Due date : {{ task.due }}
                            </br>Status : {{ task.status }}
                        </li>
            {% endfor %}
        </ul>
    </div>

{% endblock %}